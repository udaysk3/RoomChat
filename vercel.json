{
    "builds": [{
        "src": "roomchat/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" ,"runtime":"python3.10.7"}
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { 
            "distDir" : "staticfiles_build"
        }
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "roomchat/wsgi.py"
        }
    ]
}